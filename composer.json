{
    "name": "notifymehq/laravel",
    "description": "A Laravel 5 bridge for notifyme",
    "license": "MIT",
    "keywords": ["notifymehq", "notifyme", "laravel"],
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "graham-campbell/manager": "~2.0",
        "illuminate/contracts": "~5.0",
        "illuminate/support": "~5.0",
        "notifymehq/notifyme": "~1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~3.0",
        "phpunit/phpunit": "^4.7.6"
    },
    "autoload": {
        "psr-4": {
            "NotifyMeHQ\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotifyMeHQ\\Tests\\Laravel\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
